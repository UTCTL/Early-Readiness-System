<h1>Listing interested_universities</h1>

<table>
  <tr>
    <th>Student</th>
    <th>University</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interested_universities.each do |interested_university| %>
  <tr>
    <td><%= interested_university.student %></td>
    <td><%= interested_university.university %></td>
    <td><%= link_to 'Show', interested_university %></td>
    <td><%= link_to 'Edit', edit_interested_university_path(interested_university) %></td>
    <td><%= link_to 'Destroy', interested_university, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

