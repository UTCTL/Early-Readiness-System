<h1>Listing exam_subjects</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exam_subjects.each do |exam_subject| %>
  <tr>
    <td><%= exam_subject.name %></td>
    <td><%= link_to 'Show', exam_subject %></td>
    <td><%= link_to 'Edit', edit_exam_subject_path(exam_subject) %></td>
    <td><%= link_to 'Destroy', exam_subject, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

