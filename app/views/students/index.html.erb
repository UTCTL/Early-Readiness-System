<h1>Listing students</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Residency</th>
    <th>Email</th>
    <th>Birthday</th>
    <th>Gender</th>
    <th>Highschool</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @students.each do |student| %>
  <tr>
    <td><%= student.name %></td>
    <td><%= student.address1 %></td>
    <td><%= student.address2 %></td>
    <td><%= student.city %></td>
    <td><%= student.state %></td>
    <td><%= student.zip %></td>
    <td><%= student.residency %></td>
    <td><%= student.email %></td>
    <td><%= student.birthday %></td>
    <td><%= student.gender %></td>
    <td><%= student.highschool %></td>
    <td><%= link_to 'Show', student %></td>
    <td><%= link_to 'Edit', edit_student_path(student) %></td>
    <td><%= link_to 'Destroy', student, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Student', new_student_path %>
